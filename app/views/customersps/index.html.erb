<p id="notice"><%= notice %></p>

<h1>Welcome to FridgeHistory</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Email</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customersps.each do |customersp| %>
      <tr>
        <td><%= customersp.namesp %></td>
        <td><%= customersp.surnamesp %></td>
        <td><%= customersp.emailsp %></td>
        <td><%= customersp.phonesp %></td>
        <td><%= link_to 'Show', customersp %></td>
        <td><%= link_to 'Edit', edit_customersp_path(customersp) %></td>
        <td><%= link_to 'Destroy', customersp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer', new_customersp_path %>
